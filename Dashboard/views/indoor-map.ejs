    function drawBall() {
      xLocation = (tag_xcale/xroom)*xpixel+xadd;
      yLocation = -(tag_ycale/yroom)*ypixel+yadd;
      ctx.beginPath();
      ctx.arc(
        xLocation,
        yLocation,
        10,
        0,
        2 * Math.PI
      );
      ctx.fillStyle = "#2775f2";
      ctx.fill();
      ctx.closePath();
    }
    
    function draw(xLocation) {
      ctx.drawImage(a, 0, 0, canvas.width, canvas.height);
 
      socket.on('data-send-from-server', (data) => {
        tag_xcale = data.xcale;
        tag_ycale = data.ycale;
      });
      drawBall();
      window.requestAnimationFrame(draw);
    }
    init();
